# Gatekeeper API Environment Configuration
# Copy to .env and customize values

# API authentication key (for standard operations)
GATEKEEPER_API_KEY=your-api-key-here

# Human authentication key (for approve/reject operations)
GATEKEEPER_HUMAN_KEY=your-human-key-here

# Storage directory (default: ./data)
GATEKEEPER_STORAGE_DIR=./data

# Server port (default: 8000)
GATEKEEPER_PORT=8000
